<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AnaBellaApp.Web</title>
    <link rel="shortcut icon" href="/images/favicon.png" type="image/x-icon">
    <link rel="icon" href="/images/favicon.png" type="image/x-icon">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">                
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="/images/AnaMaryName.png" /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Principal</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Product" asp-action="ProductIndex">Produto</a>
                        </li>
                    </ul>

                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            @* &copy; 2025 - AnaBellaApp.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> *@
            <span class="text-black-50">Redes Sociais</span>
            <a href="https://www.facebook.com/sharer/sharer.php?u=https://www.facebook.com/anamaryboutiquemovel" target="_blank">
                <img class="_6voz _7f2d img" src="https://scontent.fssz10-1.fna.fbcdn.net/v/t39.2365-6/89126182_222315695571651_4936319991919149056_n.svg?_nc_cat=1&ccb=1-7&_nc_sid=e280be&_nc_eui2=AeHbyODbHQUFQCkS72SY7hUKbuhHtLyWjzJu6Ee0vJaPMgEVUlK4hhaLfS9NjjgG5J8_9U5ffOkHV23vnDDeQR4E&_nc_ohc=bnYzFRXbCCYQ7kNvgG52z9o&_nc_zt=14&_nc_ht=scontent.fssz10-1.fna&_nc_gid=ArMnn6i_ciuLsoDxeaNLMIt&oh=00_AYCU3DgNEkcddpYPzbCpwg09TgHGViOPEfdKGC4UT8xUtA&oe=67A089EF" 
                     alt="Siga-nos no facebook"                     
                     style="width:24px; height:auto; display:inline; margin-right:20px;" />
            </a>
            <a href="https://www.facebook.com/photo/?fbid=479635740841054&set=a.479635707507724" target="_blank">
                <img class="_6voz _7f2d img" src="https://scontent.fssz10-1.fna.fbcdn.net/v/t39.2365-6/89319900_506382610280628_2520212398984396800_n.svg?_nc_cat=1&ccb=1-7&_nc_sid=e280be&_nc_eui2=AeEH5BfPD9L9QDCzl3ATjzZGsJdQ5Sy-V7Cwl1DlLL5XsG9hIoslx_bELMaFeiqOKl6qw47StaPDHnk209UQJgTa&_nc_ohc=0Kl2ZSj1BywQ7kNvgFejWzB&_nc_zt=14&_nc_ht=scontent.fssz10-1.fna&_nc_gid=ArMnn6i_ciuLsoDxeaNLMIt&oh=00_AYCVodhRVcAROU1Lbg9mwUqsKirWzuAUEl3r2SIpBpcRhA&oe=67A09575" 
                     alt="Siga-nos no Instagram"
                     style="width:24px; height:auto; display:inline; margin-right:20px;" />
            </a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
